package HW5;

import java.text.ParseException;
import java.text.SimpleDateFormat;
import java.util.Date;

public class trytry {

	public static void main(String[] args) throws ParseException {
		Date aptdate = new SimpleDateFormat("MM/dd/yyyy").parse("1/1/2018");
		Date aptdate2 = new SimpleDateFormat("MM/dd/yyyy").parse("1/1/2018");
		boolean tf = (aptdate.equals(aptdate2));
		System.out.println(tf);
	}

}
